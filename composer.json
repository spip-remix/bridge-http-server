{
    "name": "spip/bride-http-server",
    "description": "SPIP HTTP Management",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "JamesRezo",
            "email": "james@rezo.net"
        }
    ],
    "require": {
        "symfony/http-foundation": "^6.4",
        "psr/http-server-middleware": "^1.0",
        "symfony/psr-http-message-bridge": "^6.4",
        "spip/bridge-pipeline": "dev-main",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5",
        "symfony/var-dumper": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Spip\\Bridge\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spip\\Bridge\\Http\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "repositories": {
        "spip": {
            "type": "composer",
            "url": "https://get.spip.net/composer"
        }
    }
}
