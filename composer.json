{
    "name": "spip-remix/bridge-http-server",
    "description": "SPIP HTTP Management",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "JamesRezo",
            "email": "james@rezo.net"
        }
    ],
    "require": {
        "php": ">=8.2",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "psr/http-server-middleware": "^1.0",
        "spip/bridge-pipeline": "dev-main",
        "symfony/http-foundation": "^7.0",
        "symfony/psr-http-message-bridge": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.0",
        "symfony/var-dumper": "^7.0"
    },
    "provide": {
        "league/pipeline-implementation": "1.0.0",
        "psr/http-server-middleware-implementation": "^1.0"
    },
    "repositories": {
        "spip": {
            "type": "composer",
            "url": "https://get.spip.net/composer"
        }
    },
    "autoload": {
        "psr-4": {
            "SpipRemix\\Bridge\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpipRemix\\Bridge\\Http\\Test\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.17"
        }
    }
}
